<div style="padding-left:16px">
  <h2 align="middle"><b>All Papers present in database are being shown here</b></h2>
  <div class="container">
    <div class="row">
      <div class="form-group">
        <input type="text" class="form-control" id="search" placeholder="Search for paper">
        <button  id="search-btn" class="btn9 " > Search</button>
      </div>
    </div>
  </div>
</div>










<div class="container">
    <div class="row">
          <!-- BEGIN SAMPLE TABLE PORTLET-->
      <div class="portlet light">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-cogs font-green-sharp"></i>
                                <span class="caption-subject font-green-sharp bold uppercase">Graphic Era Research Papers</span>
                            </div>
                        </div>
                        <div class="portlet-body flip-scroll">
                            <table class="table table-bordered table-striped table-condensed flip-content">
                            <thead class="flip-content">
                            <tr>

                                <th>
                                     Paper Name
                                </th>
                                <th class="numeric">
                                     Author Name
                                </th>
                                <th class="numeric">
                                    Org Published
                                </th>
                                <th class="numeric">
                                    Display
                                </th>
                                <th class="numeric">
                                    Download
                                </th>
                                <th class="numeric">
                                    Delete
                                </th>
                            </tr>
                            </thead>

                            <tr>
                              <div id="content">
                                    {{# each items }}
                                      <div class="item">


                                  <tbody>
                                                        <td class="numeric">
                                        {{PaperName }}
                                        </td>
                                        <td class="numeric">
                                         {{ Name }}
                                            </td>
                                                  <td class="numeric">
                                             {{ JournalName }}
                                              </td>



                                        <td class="numeric">
                                        <form action="/displaypdf1" method="post" target="_blank">
                                      <div class="input">
                                     <label for="id"></label>
                                           <button type="submit"  id="{{filename}}" name="{{filename}}" class="buttonload"> <i class="fa fa-desktop" aria-hidden="true"></i> &nbsp;<b>DISPLAY</b></a>


                                          </div>
                                             </form>

                                           </td>
                                           <td class="numeric">


                                             <form action="/downloadpdf1" method="post">
                                           <div class="input">
                                          <label for="id"></label>


                                               <button type="submit" id="{{ filename }}" name="{{ filename }}" class="btn"><i class="fa fa-download"></i>Download</button>
                                               </div>
                                         </form>
                                       </td>
                                               <td class="numeric">
                                                             <form action="/delete" method="post">
                                                           <div class="input">
                                                          <label for="id"></label>
                                                                <button type="submit"  id="{{filename}}" name="{{filename }} {{ _id}}" class="btn1 btn-danger" ><i class="fa fa-trash-o fa-lg"></i>&nbsp;<b>DELETE</b></a>


                                                               </div>
                                                                  </form>

                                          </td>
                                        </tr>

                                        </tbody>


                                        {{/each}}
                                    </div>
                                    </div>

                            </table>
                        </div>
                    </div>
                    <!-- END SAMPLE TABLE PORTLET-->
    </div>
</div>
</body>

<!--including jquery for ajax-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>

      let searchBtn = $('#search-btn').click(function() {
            alert( "Handler for .click() called.");
                   let searchedString = $('#search').val();
            console.log("SEARCHED STRING : ",searchedString);

            //ajax example
            $.ajax({
                    type: "POST",
                    url: "/searched",
                    data: {"search":searchedString},
                    success: function (data) {
                      console.log(data);
                      $('#content').html('');
                      let itemsfromSearch ;
                      if(data.items.length > 0)
                      {
                        itemsfromSearch =   data.items.map(item=>{
                        return(`
                          <div class="container">
                              <div class="row">
                                    <!-- BEGIN SAMPLE TABLE PORTLET-->
                                <div class="portlet light">

                            <div class="portlet-body flip-scroll">
                            <table class="table table-bordered table-striped table-condensed flip-content">
                            <thead class="flip-content">
                            <tr>

                                <th>
                                     Paper Name
                                </th>
                                <th class="numeric">
                                     Author Name
                                </th>
                                <th class="numeric">
                                    Org Published
                                </th>
                                <th class="numeric">
                                    Display
                                </th>
                                <th class="numeric">
                                    Download
                                </th>

                            </tr>
                            </thead>



                          <tr>

                                <tbody>
                                  <td class="numeric">
                                      ${item.PaperName }
                                      </td>
                                      <td class="numeric">
                                       ${item.Name }</td>

                                        <td class="numeric">
                                       ${item.JournalName }</td>

                                       <td class="numeric">

                                       <form action="/displaypdf1" method="post" target="_blank">
                                     <div class="input">
                                    <label for="id"></label>
                                          <button type="submit"  id="${item. filename}" name="${item. filename}" class="buttonload"> <i class="fa fa-desktop" aria-hidden="true"></i> &nbsp;<b>DISPLAY</b></a>


                                         </div>
                                            </form>

                                            </td>

                                            <td>
                                           <form action="/downloadpdf1" method="post">

                                            <div class="input">
                                            <label for="id"></label>
                                           <button type="submit" id="${item.filename }" name="${item. filename }" class="btn"><i class="fa fa-download"></i>Download</button>
                                           </div>
                                            </form>
                                               </td>


                             </tr>
</table>
</div>

</div>
</div>
</div>
                        `);
                       })
                      }
                      else{

                        itemsfromSearch = ["NOTHING FOUND WITH THIS NAME"]
                      }
                      $('#content').append(itemsfromSearch.join(' '));
                    },
                  });

        });

</script>

